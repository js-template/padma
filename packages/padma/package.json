{
  "name": "@padmadev/padma",
  "description": "Padma- Nextjs Framework for Strapi",
  "version": "0.1.0",
  "author": "Zelal Hossain",
  "bin": {
    "padma": "./bin/run.js"
  },
  "bugs": "https://github.com/js-template/padma",
  "dependencies": {
    "@inquirer/prompts": "^7.0.1",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.1",
    "eslint-config-next": "^15.0.3",
    "fs-extra": "^11.2.0",
    "i": "^0.3.7",
    "inquirer": "^12.0.1",
    "minimatch": "5.1.0",
    "npm": "^10.9.1",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "ajv": "^8.17.1",
    "chai": "^4",
    "eslint": "^9.15.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "/templates"
  ],
  "homepage": "https://github.com/js-template/padma/",
  "keywords": [
    "padma",
    "cli",
    "nextjs",
    "headless cms"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "padma",
    "commands": "./dist/commands",
    "dirname": "padma",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topics": {
      "create": {
        "description": " Create a new Padma project"
      }
    }
  },
  "repository": "https://github.com/js-template/padma/",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
