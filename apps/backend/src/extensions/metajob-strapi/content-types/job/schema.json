{
  "kind": "collectionType",
  "collectionName": "jobs",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "Jobs",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "type": {
      "type": "customField",
      "options": [
        "Full Time",
        "Part Time",
        "Contract Base",
        "Freelance",
        "Remote"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "vacancy": {
      "type": "integer"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "company": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "open",
        "closed",
        "draft"
      ],
      "default": "draft",
      "required": true
    },
    "endDate": {
      "type": "date",
      "required": true
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "location": {
      "type": "customField",
      "customField": "plugin::google-maps.location-picker"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
